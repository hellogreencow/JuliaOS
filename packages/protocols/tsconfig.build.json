{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "skipDefaultLibCheck": true,
    "strict": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.d.ts",
    "src/dex/test-*.ts",
    "src/dex/chains/**/*",
    "src/dex/run-*.ts",
    "src/dex/agents/cross-chain-agent.ts",
    "src/dex/swarms/cross-chain-swarm.ts"
  ]
} 